{
  "name": "kube-knots-workspace",
  "packageManager": "yarn@3.3.1",
  "private": true,
  "workspaces": [
    "app/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "yarn workspace kube-knots run tauri dev",
